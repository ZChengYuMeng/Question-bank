package www.cloudquestionbank.com.entity;

import java.sql.Timestamp;

/**
 * 公告表(编号,标题,主体,学校编号,班级编号,公告发起管理员编号,公告发起的管理员类型,公告创建时间,公告出现的时间,是否作废)
 * @author ( 人 )
 *
 */

public class Notice implements java.io.Serializable {

	// Fields

	private Integer id;
	private String title;
	private String body;
	private Integer noticeSchoolId;
	private String noticeClassId;
	private String adminAuthorId;
	private Integer adminAuthorTpeId;
	private String createTime;
	private String startTime;
	private Integer beOverdue;
	private String windosBody;
	// Constructors

	/** default constructor */
	public Notice() {
	}


	// Property accessors

	

	public String getWindosBody() {
		return windosBody;
	}





	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}


	public void setStartTime(String startTime) {
		this.startTime = startTime;
	}


	public void setWindosBody(String windosBody) {
		this.windosBody = windosBody;
	}


	public Integer getId() {
		return this.id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getTitle() {
		return this.title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getBody() {
		return this.body;
	}

	public void setBody(String body) {
		this.body = body;
	}

	public Integer getNoticeSchoolId() {
		return this.noticeSchoolId;
	}

	public void setNoticeSchoolId(Integer noticeSchoolId) {
		this.noticeSchoolId = noticeSchoolId;
	}

	

	@Override
	public String toString() {
		return "Notice [id=" + id + ", title=" + title + ", body=" + body
				+ ", noticeSchoolId=" + noticeSchoolId + ", noticeClassId="
				+ noticeClassId + ", adminAuthorId=" + adminAuthorId
				+ ", adminAuthorTpeId=" + adminAuthorTpeId + ", createTime="
				+ createTime + ", startTime=" + startTime + ", beOverdue="
				+ beOverdue + ", windosBody=" + windosBody + "]";
	}


	public String getNoticeClassId() {
		return noticeClassId;
	}


	public void setNoticeClassId(String noticeClassId) {
		this.noticeClassId = noticeClassId;
	}


	public String getAdminAuthorId() {
		return this.adminAuthorId;
	}

	public void setAdminAuthorId(String adminAuthorId) {
		this.adminAuthorId = adminAuthorId;
	}

	public Integer getAdminAuthorTpeId() {
		return this.adminAuthorTpeId;
	}

	public void setAdminAuthorTpeId(Integer adminAuthorTpeId) {
		this.adminAuthorTpeId = adminAuthorTpeId;
	}

	public String getCreateTime() {
		return createTime;
	}


	public String getStartTime() {
		return startTime;
	}


	public Integer getBeOverdue() {
		return this.beOverdue;
	}

	public void setBeOverdue(Integer beOverdue) {
		this.beOverdue = beOverdue;
	}

}